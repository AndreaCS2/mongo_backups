# MongoDB Configuration
MONGO_HOST=localhost
MONGO_PORT=27017
MONGO_USER=admin
MONGO_PASSWORD=your_password_here
MONGO_AUTH_DB=admin
MONGO_DATABASE=--all-databases
MULTI_FILES=yes

# S3 Configuration
S3_ACCESS_KEY_ID=your_access_key_here
S3_SECRET_ACCESS_KEY=your_secret_key_here
S3_BUCKET=my-mongo-backups
S3_REGION=us-east-1
S3_PREFIX=backup
S3_FILENAME=**None**
S3_ENDPOINT=**None**
S3_S3V4=no
S3_ENSURE_BUCKET_EXISTS=yes
S3_IAMROLE=false

SCHEDULE=0 2 * * *


